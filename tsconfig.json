{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "jsx": "preserve",
    "types": ["vitepress/client", "node"],
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.vue", ".vitepress/**/*"]
}
